      },
      "rdfs": {
        "uri": "http://www.w3.org/2000/01/rdf-schema#",
        "description": "RDF Schema для описания классов и иерархий",
        "usage": "rdfs:Class, rdfs:subClassOf, rdfs:label, rdfs:comment"
      },
      "owl": {
        "uri": "http://www.w3.org/2002/07/owl#",
