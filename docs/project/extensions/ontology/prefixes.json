      },
      "sh": {
        "uri": "http://www.w3.org/ns/shacl#",
        "description": "SHACL для валидации данных",
        "usage": "sh:NodeShape, sh:PropertyShape, sh:targetClass, sh:path"
      },
      "xsd": {
        "uri": "http://www.w3.org/2001/XMLSchema#",
